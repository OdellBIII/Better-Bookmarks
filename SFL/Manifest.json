{
  "name": "SFL",
  
  "version": "1.2",
  "description": "Extension that launches sites that you wanted to view later",
  "manifest_version": 2,
  
  "background": {
	"scripts": ["L2.js"]
  },
  
  "icons":  {
	"16": "SFL16.png",
	"48": "SFL48.png",
	"128": "SFL128.png"
  },
  
  "browser_action": {
    "default_icon": "SFL.png",
	"default_title": "Timmy"
  },
  
  "permissions": [
	"activeTab",
	"tabs",
	"background",
	"bookmarks",
	"alarms",
	"notifications"
  ]
}